S_START_DICEMIX -> [C_KEY_EXCHANGE]
S_KEY_EXCHANGE -> [C_EXP_DC_VECTOR]
S_EXP_DC_VECTOR -> [C_SIMPLE_DC_VECTOR]
S_SIMPLE_DC_VECTOR -> [C_TX_CONFIRMATION, C_BLAME]
S_TX_CONFIRMATION -> []

Initiate DiceMix Protocol

